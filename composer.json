{
  "name": "digitalriver/digital-river-php",
  "version": "1.0.2023",
  "description": "Client library for Digital River APIs",
  "keywords": [
    "Digital River",
    "Global Commerce",
    "Client",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://www.digitalriver.com/docs/digital-river-api-reference/",
  "license": "mit",
  "authors": [
    {
      "name": "Digital River",
      "homepage": "https://www.digitalriver.com/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "DigitalRiver\\ApiSdk\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DigitalRiver\\ApiSdk\\Test\\": "test/"
    }
  }
}
